  line-height: 28px;
  height: 36px;
  font-size: 16px;
}

#filter-toggle {
  display: none;
  font-size: 16px;
}
#filter-toggle:checked ~ .section-header-filters {
  display: block;
}

.active-filters {
  width: 100%;
}
.active-filters a {
  display: inline-block;
  padding: 3px 6px;
  background: #444;
  color: #fff;
  margin-right: 12px;
  margin-bottom: 12px;
  border-radius: 3px;
  position: relative;
  padding-right: 24px;
  text-decoration: none;
}
.active-filters a:after {
  display: inline-block;
  content: "✕";
  position: absolute;
  right: 6px;
  top: 3px;
}
.active-filters a:empty {
  display: none;
}

.section-header-filters {
  display: none;
  width: 100%;
}
.section-header-filters ul {
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 18px;
}
.section-header-filters ul li {
  border: 1px solid #444;
  border-radius: 3px;
  margin-right: 6px;
  margin-bottom: 12px;
}
.section-header-filters ul li a {
  padding: 3px 6px;
  display: inline-block;
}
.section-header-filters ul li:hover, .section-header-filters ul li.active-filter {
  background: #444;
}
.section-header-filters ul li:hover a, .section-header-filters ul li.active-filter a {
  color: #fff;
  text-decoration: none;
}

/*============================================================================
	#Breadcrumbs
==============================================================================*/
.breadcrumb {
  margin-top: calc(var(--gutter) / 2);
  margin-bottom: var(--gutter);
  font-size: calc(var(--base_font_size) - 3px);
}
.breadcrumb a,
.breadcrumb span {
  display: inline-block;
  padding: 0 var(--gutter)/4 0 0;
}
.breadcrumb a:first-child,
.breadcrumb span:first-child {
  padding-left: 0;
}

/*============================================================================
	#Product Page
==============================================================================*/
.product-variants {
  display: none;
}
.no-js .product-variants {
  display: block;
}

.rating-wrapper {
  font-size: 1.6rem;
}
.rating-wrapper .active:before {
  color: var(--color_primary);
}

.key-features li:before {
  content: "✓";
  margin-right: 10px;
}

.key-features {
  margin-top: calc(var(--gutter) / 2);
}
.key-features li {
  font-size: 1.6rem;
}

.review-box {
  border: 1px solid var(--color_borders);
  border-radius: var(--radius);
  padding: var(--gutter);
  margin-bottom: var(--gutter);
}
.review-box hr {
  margin: calc(var(--gutter) / 2) 0;
}

@media screen and (min-width: {{ settings.breakpoint_medium_ending | plus: 1 }}px) {
  .selector-wrapper select,
.product-variants select {
    max-width: 40%;
  }
}
.product-photo-container {
  margin-bottom: var(--gutter);
}

.product-photo-container a, 
.product-photo-container img,
.product-photo-thumbs a,
.product-photo-thumbs img {
  display: block;
  margin: 0 auto;
}
.product-photo-container li,
.product-photo-thumbs li {
  margin-bottom: var(--gutter);
}

.product-review-count {
  white-space: nowrap;
}

.review {
  margin-bottom: var(--gutter);
}
.review + .review {
  border-top: 1px solid var(--color_borders);
  padding-top: var(--gutter);
}

/*============================================================================
	#Blogs and Comments
==============================================================================*/
.article-card h3, .article-card p, .article-card .rte {
    margin:0;
}

article {
  background-color: var(--color_body_bg);
}

.comment {
  margin-bottom: var(--gutter);
}
.comment + .comment {
  border-top: 1px solid var(--color_borders);
  padding-top: var(--gutter);
}

/*============================================================================
	#Notes and Form Feedback
==============================================================================*/
.note,
.errors {
  border-radius: var(--radius);
  padding: calc(var(--gutter) / 2);
  margin-bottom: calc(var(--gutter) / 2);
  border: 1px solid transparent;
}
.note ul,
.note ol,
.errors ul,
.errors ol {
  margin-top: 0;
  margin-bottom: 0;
}
.note li:last-child,
.errors li:last-child {
  margin-bottom: 0;
}
.note p,
.errors p {
  margin-bottom: 0;
}

.note {
  border-color: var(--color_borders);
}

.errors ul {
  list-style: disc outside;
  margin-left: 20px;
}

.form-success {
  color: var(--successGreen);
  filter: brightness(125%);
  border-color: var(--successGreen);
}

.form-error,
.errors {
  color: var(--errorRed);
  filter: brightness(155%);
  border-color: var(--errorRed);
}

/*============================================================================
	#Pages
==============================================================================*/
.template-page h1, .template-page .h1 {
  text-align: var(--headings_alignment);
  margin-top: calc(var(--gutter) * 2);
}
.template-page .main-content {
  padding: 1.8rem;
}

/*============================================================================
	#Cart Page
==============================================================================*/
.cart-row {
  margin-top: var(--gutter);
  padding-top: var(--gutter);
  border-top: 1px solid var(--color_borders);
}
.cart-row:first-child {
  margin-top: 0;
}
.cart-row:first-child {
  padding-top: 0;
  border-top: 0 none;
}
.cart-row a {
  color: var(--color_body_text);
}
.cart-row a:hover {
  color: var(--color_primary);
}

.cart-subtotal {
  margin-bottom: var(--gutter);
}

.cart-subtotal--price {
  margin: 0 0 0 var(--gutter)/3;
  display: inline;
}

.quantity-selector-wrapper {
  margin-bottom: calc(var(--gutter) / 2);
  max-width: 140px;
}
.quantity-selector-wrapper .quantity-selector {
  text-align: center;
  border-right: none;
  border-left: none;
  min-width: 50px;
  border-radius:0;
}
.quantity-selector-wrapper .btn, .quantity-selector-wrapper .btn-secondary, .quantity-selector-wrapper input[type=submit] {
  width: 37px;
  text-align: center;
  background-color: #fff;
  color: #000000;
  border: 1px solid var(--color_borders);
}

@media screen and (max-width: {{ settings.breakpoint_small }}px) {
  .template-cart .quantity-selector-wrapper input {
    min-width: 0px;
  }

  .template-cart .quantity-selector-wrapper .btn, .template-cart .quantity-selector-wrapper .btn-secondary, .template-cart .quantity-selector-wrapper input[type=submit] {
    width: auto;
    padding-left: 7px;
    padding-right: 7px;
    border: 0;
  }
}
label.show_terms {
  display: inline-block;
  color: #888;
  text-decoration: underline;
}

.terms-content {
  position: fixed;
  flex-direction: column;
  z-index: 99;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  background: rgba(0, 0, 0, 0.5);
  padding: 24px;
}
.terms-content > div {
  white-space: pre-line;
  overflow: scroll;
  text-align: left;
  width: 100%;
  max-width: 640px;
  margin: 0 auto;
  padding: 24px;
  background: #fff;
  font-size: 1.6rem;
  line-height: 1.34;
  font-weight: normal;
}
.terms-content > div:nth-child(2) {
  padding-top: 0;
}
.terms-content > div svg {
  transform: translateY(-16px);
}

#show_terms:checked ~ .terms-content {
  display: flex !important;
}

/*==============================================================
    #Social Sharing
================================================================*/
/*=== Social share buttons ===*/
.social-sharing {
  font-family: "HelveticaNeue", "Helvetica Neue", Helvetica, Arial, sans-serif;
  padding-top: calc(var(--gutter) / 2);
}
.social-sharing a {
  display: inline-block;
  color: #fff;
  border-radius: 2px;
  margin: 0 10px 10px 0;
  height: 22px;
  line-height: 22px;
  text-decoration: none;
  font-weight: normal;
}
.social-sharing a:hover {
  color: #fff;
}
.social-sharing span {
  display: inline-block;
  vertical-align: top;
  height: 22px;
  line-height: 22px;
  font-size: 12px;
}
.social-sharing i {
  padding: 0 5px 0 10px;
}
.social-sharing i:before {
  line-height: 22px;
}

.share-title {
  font-weight: 900;
  font-size: 12px;
  padding-right: 10px;
}

.share-facebook {
  background-color: #3b5998;
}
.share-facebook:hover {
  background-color: #2d4373;
}

.share-fancy {
  background-color: #4999dc;
}
.share-fancy:hover {
  background-color: #2780cb;
}

.share-twitter {
  background-color: #00aced;
}
.share-twitter:hover {
  background-color: #0087ba;
}

.share-pinterest {
  background-color: #cb2027;
}
.share-pinterest:hover {
  background-color: #9f191f;
}

.share-google {
  background-color: #dd4b39;
}
.share-google:hover {
  background-color: #c23321;
}

.share-count {
  position: relative;
  background-color: #fff;
  padding: 0 8px;
  margin-right: -2px;
  font-size: 14px;
  line-height: 20px;
  color: #333;
  border-radius: 0 2px 2px 0;
  border: 1px solid #ececec;
  opacity: 0;
  -webkit-transition: opacity 0.2s ease-in;
  -moz-transition: opacity 0.2s ease-in;
  -ms-transition: opacity 0.2s ease-in;
  -o-transition: opacity 0.2s ease-in;
  transition: opacity 0.2s ease-in;
}
.share-count.is-loaded {
  opacity: 1;
}
.share-count:before, .share-count:after {
  content: "";
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -6px;
  width: 0;
  height: 0;
  border-top: 6px solid transparent;
  border-bottom: 6px solid transparent;
  border-right: 6px solid;
}
.share-count:before {
  left: -6px;
  border-right-color: #ececec;
}
.share-count:after {
  left: -5px;
  border-right-color: #fff;
}
a:hover .share-count {
  background-color: whitesmoke;
}
a:hover .share-count:after {
  border-right-color: whitesmoke;
}

/*==============================================================
    #Checkout and Thank you page
================================================================*/
#checkout_payment_methods li {
  margin: calc(var(--gutter) / 2) 0px;
}
#checkout_payment_methods li img {
  vertical-align: middle;
}

#checkout .price_summary {
  display: block;
  line-height: 2.5rem;
  text-align: right;
}
#checkout .price_summary strong {
  font-size: 1.4rem;
}
#checkout .price_summary span {
  float: left;
  font-size: 1.4rem;
  color: var(--color_body_text);
  filter: brightness(120%);
}

#checkout .price_summary.price_grandtotal {
  font-size: 1.8rem;
}

#checkout fieldset {
  margin-bottom: var(--gutter);
}

#pay-btn {
  font-size: 2.2rem;
  color: #fff;
  background-color: var(--successGreen);
  border-color: var(--successGreen);
  filter: brightness(90%);
}

/*==============================================================
    #Modal
================================================================*/
@media screen and (max-width: {{ settings.breakpoint_small }}px) {
  body.modal--visible {
    overflow: hidden;
  }
}
.modal {
  position: absolute;
  background-color: #fff;
  top: 170px;
  left: -200%;
  margin-left: -500px;
  width: 100%;
  max-width: 760px;
  height: auto;
  z-index: 99999999;
  visibility: hidden;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  -webkit-transition: left 0s linear 700ms, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out;
  -moz-transition: left 0s linear 700ms, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out;
  -ms-transition: left 0s linear 700ms, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out;
  -o-transition: left 0s linear 700ms, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out;
  transition: left 0s linear 700ms, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out;
}
@media only screen and (max-height: 645px) {
  .modal {
    top: 70px;
  }
}
@media screen and (max-width: {{ settings.breakpoint_medium_ending }}px) {
  .modal {
    max-width: 90%;
  }
}
.modal .modal--close {
  position: absolute;
  cursor: pointer;
  right: var(--gutter);
  top: var(--gutter);
  font-size: 1.6rem;
}
@media screen and (max-width: {{ settings.breakpoint_small }}px) {
  .modal {
    position: fixed;
    top: 0;
    bottom: 0;
    margin: 0 !important;
    max-height: none;
    max-width: none;
  }
  .modal .modal--close {
    display: block;
    text-align: right;
    position: relative;
    right: auto;
    left: 0;
    top: 0;
  }
}
.modal .modal--content {
  position: relative;
  padding: var(--gutter);
}
@media screen and (max-width: {{ settings.breakpoint_medium_ending | plus: 1 }}px) {
  .modal .modal--content {
    padding-left: 15px;
    padding-right: 15px;
  }
}
@media screen and (max-width: {{ settings.breakpoint_small }}px) {
  .modal .modal--content {
    padding-bottom: 60px;
    min-height: 100%;
  }
}
.modal.is-visible {
  visibility: visible;
  left: 50%;
  -webkit-transition: left 0s linear, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out, opacity 400ms cubic-bezier(0.47, 0.5, 0, 0.95), transform 600ms cubic-bezier(0.47, 0.5, 0, 0.95);
  -moz-transition: left 0s linear, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out, opacity 400ms cubic-bezier(0.47, 0.5, 0, 0.95), transform 600ms cubic-bezier(0.47, 0.5, 0, 0.95);
  -ms-transition: left 0s linear, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out, opacity 400ms cubic-bezier(0.47, 0.5, 0, 0.95), transform 600ms cubic-bezier(0.47, 0.5, 0, 0.95);
  -o-transition: left 0s linear, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out, opacity 400ms cubic-bezier(0.47, 0.5, 0, 0.95), transform 600ms cubic-bezier(0.47, 0.5, 0, 0.95);
  transition: left 0s linear, box-shadow 600ms cubic-bezier(0.47, 0.5, 0, 0.95) 400ms, margin-top 200ms ease-in-out, opacity 400ms cubic-bezier(0.47, 0.5, 0, 0.95), transform 600ms cubic-bezier(0.47, 0.5, 0, 0.95);
}
@media screen and (min-width: {{ settings.breakpoint_medium_ending | plus: 1 }}px) {
  .modal.is-visible {
    box-shadow: 0px 10px 80px rgba(0, 0, 0, 0.25);
  }
}
@media screen and (max-width: {{ settings.breakpoint_small }}px) {
  .modal.is-visible {
    left: 0;
  }
}
.modal.no-transforms {
  border: 1px solid var(--color_borders);
}

#modal-overlay {
  position: fixed;
  width: 100%;
  height: 100%;
  visibility: hidden;
  top: 0;
  left: 0;
  z-index: 99999998;
  opacity: 0;
  cursor: pointer;
  -webkit-transition: all 450ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  -moz-transition: all 450ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  -ms-transition: all 450ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  -o-transition: all 450ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  transition: all 450ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  background-color: #d3d3d3;
}

@media screen and (min-width: {{ settings.breakpoint_small | plus: 1 }}px) {
  .modal--visible #modal-overlay {
    visibility: visible;
    -webkit-transition: all 650ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
    -moz-transition: all 650ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
    -ms-transition: all 650ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
    -o-transition: all 650ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
    transition: all 650ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
    opacity: 0.8;
  }
  .lt-ie9 .modal--visible #modal-overlay {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
  }
}
.modal {
  display: none;
  -webkit-transform: scale(0.9);
  -moz-transform: scale(0.9);
  -ms-transform: scale(0.9);
  -o-transform: scale(0.9);
  transform: scale(0.9);
  opacity: 0;
}

.modal.is-visible {
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  -o-transform: scale(1);
  transform: scale(1);
  opacity: 1;
}

/*==============================================================
    #Placeholders
================================================================*/
.placeholder {
  background: #fafafa;
  display: flex;
  align-items: center;
  justify-content: center;
}
.placeholder svg {
  fill: #666;
  width: 100%;
  height: 100%;
  max-width: 50%;
  max-height: 50%;
}
