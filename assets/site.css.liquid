  background-color: var(--color_footer_bg);
  box-sizing: border-box;
  font-size: 62.5%;
}

*, *:before, *:after {
  box-sizing: inherit;
}

body {
  position: relative;
  background-color: var(--color_body_bg);
  font-size: var(--base_font_size, 12px);
  transition: all 0.5s ease;
}

.modal-background {
  display: none;
}

